{
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "test-e2e": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.ts'",
    "test-unit": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register -r tsconfig-paths/register 'src/**/*.test.ts'",
    "test-coverage": "nyc --reporter=text env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register -r tsconfig-paths/register 'src/**/*.test.ts'",
    "test": "web-test-runner \"src/**/*.test.tsx\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "npx eslint --fix \"src/**/*.{js,jsx,ts,tsx}\" && npx stylelint --fix \"src/**/*.{css,scss}\"",
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
